{
  "name": "jest-intro",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/dlrandy/jest-intro.git",
  "author": "dlrandy <1208484996@qq.com>",
  "license": "MIT",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "dev": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "serve --no-clipboard --single --listen 8080 dist",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|css|html|md)\"",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm run test && npm run build",
    "setup": "npm install && npm run validate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/jest": "11.2.1",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.3.0",
    "emotion-theming": "11.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@babel/runtime": "7.13.10",
    "@emotion/babel-preset-css-prop": "11.2.0",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "babel-loader": "8.2.2",
    "css-loader": "5.2.0",
    "eslint": "7.23.0",
    "eslint-config-cypress": "0.28.0",
    "eslint-config-kentcdodds": "17.6.0",
    "eslint-import-resolver-jest": "3.0.0",
    "file-loader": "6.2.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "is-ci-cli": "2.2.0",
    "jest": "26.6.3",
    "jest-runner-eslint": "0.10.0",
    "jest-watch-select-projects": "2.0.0",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prop-types": "15.7.2",
    "serve": "11.3.2",
    "style-loader": "2.0.0",
    "webpack": "5.30.0",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  }
}
